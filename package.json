{
  "name": "github-traffic",
  "description": "Get the Github traffic for the specified repository",
  "version": "0.1.2",
  "homepage": "https://github.com/doowb/github-traffic",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "doowb/github-traffic",
  "bugs": {
    "url": "https://github.com/doowb/github-traffic/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "LICENSE"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha test"
  },
  "dependencies": {
    "co": "^4.6.0",
    "extend-shallow": "^2.0.1",
    "get-repository-url": "^0.1.1",
    "github-base": "^0.4.1",
    "parse-github-url": "^0.3.2"
  },
  "devDependencies": {
    "data-store": "^0.16.1",
    "gulp-format-md": "^0.1.10",
    "mocha": "^3.0.2",
    "yargs-parser": "^3.2.0"
  },
  "keywords": [
    "api",
    "base",
    "github",
    "github-api",
    "github-base",
    "github-traffic",
    "traffic"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "github-base",
        "parse-github-url",
        "get-repository-url"
      ]
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
